{
    "version":1,
    "disable_existing_loggers":false,
    "formatters":{
       "json":{
          "()":"logger.MyJSONFormatter",
          "datefmt":"%Y-%m-%dT%H:%M:%S%z",
          "max_length":1024,
          "fmt_keys":{
             "level":"levelname",
             "timestamp":"asctime",
             "message":"msg",
             "extra":"extra",
             "logger":"name",
             "module":"module",
             "function":"funcName",
             "line":"lineno"
          }
       }
    },
    "handlers":{
       "console":{
          "class":"logging.StreamHandler",
          "level":"DEBUG",
          "formatter":"json",
          "stream":"ext://sys.stdout"
       },
       "file":{
          "class":"logging.handlers.RotatingFileHandler",
          "level":"DEBUG",
          "formatter":"json",
          "filename":"logs/my_app.log.jsonl",
          "maxBytes":1000000,
          "backupCount":3
       }
    },
    "loggers":{
       "":{
          "level":"DEBUG",
          "handlers":[
             "console",
             "file"
          ]
       }
    }
 }